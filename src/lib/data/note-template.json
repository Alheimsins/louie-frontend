{
  "template": {
    "header":[],
    "watermark": {
      "text": "Forhåndsvisning",
      "color": "red",
      "opacity": 0.3
    },
    "content":[
      {
        "columns":[
          {
            "image":"logo",
            "fit":[
              50,
              50
            ],
            "width":40
          },
          {
            "text":"${companyName}",
            "style": "h3"
          }
        ]
      },
      {
        "alignment":"justify",
        "columns":[
          {
            "text":"${studentName}",
            "margin":[0, 20, 0, 90]
          },
          {
            "text":[
              {
                "text":"/ ${schoolName}\n\n",
                "style":"h2"
              },
              "Vår dato: ${date}\n",
              "Unntatt offentlighet iht\n",
              "Offl §13 jfr Fvl §13"
            ]
          }
        ]
      },
      {
        "text":"Lærernotat - ${studentName}\n\n",
        "style":"h1"
      },
      "${note}"
    ],
    "defaultStyle":{
      "columnGap":20,
      "fontSize": 11,
      "font": "Times"
    },
    "styles": {
      "h1": {
        "fontSize": 17,
        "bold": true,
        "font": "Roboto"
      },
      "h2": {
        "fontSize": 15,
        "bold": true
      },
      "h3": {
        "fontSize":27,
        "font": "Roboto",
        "margin":[0, 16, 0, 0]
      }
    },
    "images":{
      "logo":"${companyLogo}"
    }
  }
}
